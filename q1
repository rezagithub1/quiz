#include <iostream>
using namespace std;
int main(void)
{
	string a;
	cin>>a;
	while(true)
	{
		for(int i=0;i<=3;i++)
		{
			cout<<a[i];
			if(i!=3)
			{
				cout<<".";
			}
			
		}
		cout<<endl;
		
		for(int i=1;i<=4;i++)
		{
			cout<<a[i];
			if(i!=4)
			{
				cout<<".";
			}
		}
		cout<<endl;
		for(int i=2;i<=5;i++)
		{
			cout<<a[i];
			if(i!=5)
			{
				cout<<".";
			}
		}
		cout<<endl;

		for(int i=3;i<=6;i++)
		{
			cout<<a[i];
			if(i!=6)
			{
				cout<<".";
			}
		}
		cout<<endl;
		for(int i=4;i<=7;i++)
		{
			cout<<a[i];
			if(i!=7)
			{
				cout<<".";
			}
		}
		cout<<endl;
		for(int i=5;i<=8;i++)
		{
			cout<<a[i];
			if(i!=8)
			{
				cout<<".";
			}
		}
		cout<<endl;
		for(int i=6;i<=9;i++)
		{
			cout<<a[i];
			if(i!=9)
			{
				cout<<".";
			}
		}
		cout<<endl;
		for(int i=7;i<=10;i++)
		{
			cout<<a[i];
			if(i!=10)
			{
				cout<<".";
			}
		}
		cout<<endl;
		for(int i=8;i<=11;i++)
		{
			cout<<a[i];
			if(i!=11)
			{
				cout<<".";
			}
		}
		for(int i=9;i<=12;i++)
		{
			cout<<a[i];
			if(i!=12)
			{
				cout<<".";
			}
		}
		cout<<endl;

		
		break;
	
	}
}
